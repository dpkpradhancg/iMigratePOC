            </ns:ActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="REST Dispatch and Reply">
        <pd:type>com.tibco.plugin.json.rest.server.activities.RestAdapterActivity</pd:type>
        <pd:resourceType>rest.activities.RestAdapter</pd:resourceType>
        <pd:x>488</pd:x>
        <pd:y>84</pd:y>
        <config>
            <eventSource>HTTP Receiver</eventSource>
            <RestService>
                <InnerService>
                    <ns0:application xmlns:ns0="http://wadl.dev.java.net/2009/02" name="addApplication">
                        <ns0:resources name="addService" base="http://localhost:7272">
                            <ns0:resource id="addResource" path="calc">
                                <ns0:doc/>
                                <ns0:method id="AddNumbers" name="GET">
                                    <ns0:doc/>
                                    <Binding process="/ServiceImplementation.process">
                                        <RESTResponseHeader>
                                            <xsd:element name="Headers">
                                                <xsd:complexType>
                                                    <xsd:sequence>
                                                        <xsd:element name="StatusLine" type="xsd:string" minOccurs="0"/>
                                                        <xsd:element name="Content-Type" type="xsd:string" minOccurs="0"/>
                                                        <xsd:element name="Location" type="xsd:string" minOccurs="0"/>
                                                        <xsd:element name="Set-Cookie" type="xsd:string" minOccurs="0"/>
                                                        <xsd:element name="Pragma" type="xsd:string" minOccurs="0"/>
                                                    </xsd:sequence>
                                                </xsd:complexType>
                                            </xsd:element>
                                        </RESTResponseHeader>
                                        <Output-Binding xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:pfx="http://www.tibco.com/schemas/iMigratePOC/Schema.xsd" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                                            <xsl:template xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:pfx="http://www.tibco.com/schemas/iMigratePOC/Schema.xsd" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsd="http://www.w3.org/2001/XMLSchema" match="/*">
                                                <RESTResponse>
                                                    <xsl:if test="$End/pfx:response">
                                                        <asciiContent>
                                                            <xsl:value-of select="$End/pfx:response"/>
                                                        </asciiContent>
                                                    </xsl:if>
                                                </RESTResponse>
                                            </xsl:template>
                                        </Output-Binding>
                                        <Input-Binding xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:ns="http://www.tibco.com/pe/WriteToLogActivitySchema" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                                            <xsl:template xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:ns="http://www.tibco.com/pe/WriteToLogActivitySchema" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:ns1="http://www.tibco.com/schemas/iMigratePOC/Schema.xsd">
                                                <ns1:request>
                                                    <ns1:number1>
                                                        <xsl:value-of select="$HTTP-Receiver/ProcessStarterOutput/parameters/number1"/>
                                                    </ns1:number1>
                                                    <ns1:number2>
                                                        <xsl:value-of select="$HTTP-Receiver/ProcessStarterOutput/parameters/number2"/>
                                                    </ns1:number2>
                                                </ns1:request>
                                            </xsl:template>
                                        </Input-Binding>
                                    </Binding>
                                </ns0:method>
                            </ns0:resource>
                            <ns0:resource id="Resource1" path="calcc">
                                <ns0:doc/>
                                <ns0:method id="Method2" name="GET">
                                    <ns0:doc/>
                                    <Binding process="/serviceImplementationSUBTRACT.process">
                                        <RESTResponseHeader>
                                            <xsd:element name="Headers">
                                                <xsd:complexType>
                                                    <xsd:sequence>
                                                        <xsd:element name="StatusLine" type="xsd:string" minOccurs="0"/>
                                                        <xsd:element name="Content-Type" type="xsd:string" minOccurs="0"/>
                                                        <xsd:element name="Location" type="xsd:string" minOccurs="0"/>
                                                        <xsd:element name="Set-Cookie" type="xsd:string" minOccurs="0"/>
                                                        <xsd:element name="Pragma" type="xsd:string" minOccurs="0"/>
                                                    </xsd:sequence>
                                                </xsd:complexType>
                                            </xsd:element>
                                        </RESTResponseHeader>
                                        <Input-Binding xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:ns="http://www.tibco.com/pe/WriteToLogActivitySchema" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                                            <xsl:template xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:ns="http://www.tibco.com/pe/WriteToLogActivitySchema" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:ns1="http://www.tibco.com/schemas/iMigratePOC/Schema.xsd" match="/*">
                                                <ns1:request>
                                                    <ns1:number1>
                                                        <xsl:value-of select="$HTTP-Receiver/ProcessStarterOutput/parameters/number1"/>
                                                    </ns1:number1>
                                                    <ns1:number2>
                                                        <xsl:value-of select="$HTTP-Receiver/ProcessStarterOutput/parameters/number2"/>
                                                    </ns1:number2>
                                                </ns1:request>
                                            </xsl:template>
                                        </Input-Binding>
                                        <Output-Binding xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:pfx="http://www.tibco.com/schemas/iMigratePOC/Schema.xsd" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                                            <xsl:template xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:pfx="http://www.tibco.com/schemas/iMigratePOC/Schema.xsd" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsd="http://www.w3.org/2001/XMLSchema" match="/*">
                                                <RESTResponse>
                                                    <xsl:if test="$End/pfx:response">
                                                        <asciiContent>
                                                            <xsl:value-of select="$End/pfx:response"/>
                                                        </asciiContent>
                                                    </xsl:if>
                                                </RESTResponse>
                                            </xsl:template>
                                        </Output-Binding>
                                    </Binding>
                                </ns0:method>
                            </ns0:resource>
                        </ns0:resources>
                    </ns0:application>
                </InnerService>
            </RestService>
        </config>
        <pd:inputBindings>
            <ns0:ActivityInputClass xmlns:ns0="http://www.tibco.com/namespaces/tnt/plugins/json">
                <tmp>
                    <xsl:value-of select="$HTTP-Receiver/ProcessStarterOutput/Method"/>
                </tmp>
            </ns0:ActivityInputClass>
        </pd:inputBindings>
